<table class="record_properties table table-condensed col-lg-12 col-md-12 col-sm-12" id="tabla_reporte">
    <thead>
            <tr>
                <th colspan="2"> Denominacion Comercial: {{ entity.denominacionComercial }}</th>
            </tr>
    </thead>
    <tbody>
        <tr>
            <td style="width: 60%; text-align: right;"> RIF </td>
            <th>
                {% if entity.empresa is defined %}
                    {{ entity.empresa.persJuridica }} - {{ entity.empresa.rif }}
                {% endif %}
            </th>
        </tr>
        <tr>
            <td style="width: 60%; text-align: right;"> Fecha Registro </td>
            <th>{{ entity.fechaRegistro|date('d-m-Y') }}</th>
        </tr>
   </tbody>
</table>
