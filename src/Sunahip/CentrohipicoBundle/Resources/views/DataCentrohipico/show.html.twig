    <div id="dataoper"  class="col-md-12">
        <table class="table table-condensed col-lg-12 col-md-12 col-sm-12" id="tabla_reporte2">
        <thead>
            <tr>
                <th colspan="4" >DATOS DEL ESTABLECIMIENTO - INFORMACION SOBRE EL LUGAR DE EXPLOTACION DEL JUEGO</th>
            </tr>
        </thead>
        <tbody>
           <tr>
                <td>Denominación Comercial:</td>
                <th colspan="3">{{ entity.denominacionComercial}}</th>
            </tr>
            <tr>
                <td>Nombre de la Empresa o persona natural/</td>
                <th>{{entity.propietarioLocal }}</th>
                <td>Rif</td>
                <th>{{ entity.persJuridica }}-{{ entity.rif }}</th>
            </tr>
            <tr>
                <td>Nombre:</td>
                <th>{{ entity.nombre }}</th>
                <td>Apellido:</td>
                <th>{{ entity.apellido }}</th>
            </tr>
            <tr>
                 <td>Estado:</td>
                
                <th>{%if entity.estado|length  > 0%}
                        {{ entity.estado.nombre }}
                    {%endif%}
                </th>
                <td>Ciudad:</td>
                <th>{%if entity.ciudad|length  > 0%}
                        {{ entity.ciudad }}
                    {%endif%}
                </th>
            </tr>
            <tr>
                <td>Municipio:</td>
                <th>{%if entity.municipio|length  > 0%}
                        {{ entity.municipio.nombre}}
                    {%endif%}
                </th>
                <td>Urbanización/Sector:</td>
                <th>{{ entity.urbanSector }}</th>
            </tr>
            <tr>
                <td>Av/Calle/Carrera:</td>
                <th>{{ entity.avCalleCarrera }}</th>
                <td>Edif/Casa:</td>
                <th>{{ entity.edifCasa }}</th>
            </tr>
            <tr>
                <td>Oficina/Apto/No:</td>
                <th>{{ entity.ofcAptoNum }}</th>
                <td>Punto Referencia</td>
                <th>{{ entity.puntoReferencia }}</th>
            </tr>
            <tr>
                <td>Teléfono Fijo:</td>
                <th>{{ entity.codTlfFijo }} {{ entity.tflFijo }}</th>
                <td>Teléfono Celular</td>
                <th>{{ entity.codTlfCelular }} {{ entity.tflCelular }}</th>
            </tr>
        </tbody>
    </table>
    </div>
    <div id="licencia"  class="col-md-12">
        <table id="tabla_reporte3" class="table table-condensed col-lg-12 col-md-12 col-sm-12">
            <thead>
                <tr>
                    <th colspan="4" > Detalles de Licencia </th>
                </tr>  
            </thead>
                <tr id="table_header">
                    <td><a href="#">Nº Licencia</a></td>
                    <td><a href="#">Clase de Licencia</a></td>
                    <td><a href="#">Fecha de Vencimiento</a></td>
                    <td><a href="#">Estatus</a></td>
                </tr>
             <tbody>
             {% if entity.licenciasaprob is defined %}
              {% for lic in entity.licenciasaprob %}  
                <tr>
                    <td class="text-center">{{lic.numLicencia}}</td>
                    <td class="text-center">{{lic.clasfLicencia}}</td>
                    <td class="text-center">{{lic.fechaVencimiento}}</td>
                    <td class="text-center">{{ lic.vigente==1?'Vigente':'Vencida' }}</td>
                </tr>
             {%endfor %}
             {% if entity.licenciasaprob is empty %}
                 <tr><td colspan="4">Sin Registros</td></tr>
             {%endif%}
             {% endif %}
          </tbody>
        </table>
    </div>
</div>
<script type="text/javascript" >
    $(document).ready(function(){
        $('#tabs').tabs();
        $("#dataoper #datalegal").show();
    });
</script>  
