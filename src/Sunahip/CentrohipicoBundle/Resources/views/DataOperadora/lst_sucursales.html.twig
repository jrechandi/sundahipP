{% extends 'CentrohipicoBundle::centroh_base.html.twig' %} 
{% block content_content %}
    	 <div class="block-separator col-sm-12"></div> 
            <div class="col-md-12">
	       <h1 class="tit_principal">Datos Legales</h1>
            </div>    
    <div class="row col-lg-12">
        <form action="" method="POST"  >  
            <select id="sucursal" name="sucursal" >   
                <option>Seleccione para Desincorporar</option>
                {% for entity in entities %}
                    <option value="">{{loop.index}} - {{ entity.denominacionComercial|upper }} </option>
                {% endfor %}
            </select>  
         <a href="#" class="btn btn-warning btn-sm" onclick="confirmaDel('{{entity.id}}','{{ entity.denominacionComercial|upper }}')"><i class="fa fa-times"></i> Desincorporar</a>
        </form>  
    </div>
{% endblock %}

